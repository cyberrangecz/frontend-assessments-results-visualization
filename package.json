{
  "name": "@muni-kypo-crp/assessment-visualization",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint && npm run prettier-check",
    "e2e": "ng e2e",
    "postinstall": "ngcc",
    "prettier-check": "prettier --check \"projects/**/*.ts\"",
    "prettier-fix": "prettier --write \"./projects/**/*.ts\"",
    "ci-test": "ng test --browsers ChromeHeadless --watch=false",
    "build-example-app": "ng build kypo2-assessments-results-visualization --prod",
    "build-library": "ng build kypo2-assessments-results-visualization-lib --prod",
    "create-package": "cd dist/kypo2-assessments-results-visualization-lib && npm pack",
    "build-and-pack": "npm install && npm run build-library && npm run create-package",
    "ci-update-version": "cd ./projects/kypo2-assessments-results-visualization-lib/ && npm version $VERSION",
    "ci-build-and-pack": "npm ci --cache .npm --prefer-offline && npm run build-library && npm run create-package",
    "ci-publish-package": "cd dist/kypo2-assessments-results-visualization-lib && npm publish"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run prettier-check"
    }
  },
  "private": true,
  "dependencies": {
    "@angular-devkit/build-angular": "~0.1000.0",
    "@angular/animations": "^10.0.1",
    "@angular/cdk": "^10.0.0",
    "@angular/common": "^10.0.1",
    "@angular/compiler": "^10.0.1",
    "@angular/core": "^10.0.1",
    "@angular/forms": "^10.0.1",
    "@angular/material": "^10.0.0",
    "@angular/platform-browser": "^10.0.1",
    "@angular/platform-browser-dynamic": "^10.0.1",
    "@angular/router": "^10.0.1",
    "@angular-eslint/builder": "^0.8.0-beta.0",
    "@angular-eslint/eslint-plugin": "^0.8.0-beta.0",
    "@angular-eslint/eslint-plugin-template": "^0.8.0-beta.0",
    "@angular-eslint/schematics": "^0.8.0-beta.0",
    "@angular-eslint/template-parser": "^0.8.0-beta.0",
    "@typescript-eslint/parser": "^4.3.0",
    "@typescript-eslint/eslint-plugin": "^4.7.0",
    "eslint": "^7.13.0",
    "@sentinel/auth": "^10.0.0",
    "angular-oauth2-oidc": "^9.2.2",
    "angular-oauth2-oidc-jwks": "^9.0.0",
    "core-js": "^2.5.4",
    "rxjs": "^6.5.4",
    "tslib": "^2.0.0",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-ng-packagr": "~0.1000.0",
    "@angular/cli": "~10.0.0",
    "@angular/compiler-cli": "^10.0.1",
    "@angular/language-service": "^10.0.1",
    "@types/jasmine": "~2.8.6",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "^12.11.1",
    "jasmine-core": "~3.5.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~5.0.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.2",
    "karma-jasmine": "~3.3.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "husky": "^4.2.5",
    "prettier": "2.0.4",
    "ng-packagr": "^10.0.0",
    "protractor": "~7.0.0",
    "ts-node": "~5.0.1",
    "typescript": "~3.9.5"
  }
}
